{
  "name": "@rationalbloks/frontblok-components",
  "version": "0.2.1",
  "description": "Reusable React components for RationalBloks frontends - Auth views, Settings, Error handling, Modals, Theme",
  "author": "RationalBloks Team",
  "license": "MIT",
  "private": false,
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "vite build && tsc --emitDeclarationOnly --outDir dist",
    "prepublishOnly": "npm run build",
    "typecheck": "tsc --noEmit"
  },
  "peerDependencies": {
    "@mui/material": "^5.0.0 || ^6.0.0 || ^7.0.0",
    "@mui/icons-material": "^5.0.0 || ^6.0.0 || ^7.0.0",
    "@emotion/react": "^11.0.0",
    "@emotion/styled": "^11.0.0",
    "@react-oauth/google": "^0.12.0",
    "@rationalbloks/frontblok-auth": "^0.3.0 || ^0.4.0",
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0",
    "react-router-dom": "^6.0.0 || ^7.0.0"
  },
  "devDependencies": {
    "@mui/material": "^6.0.0",
    "@mui/icons-material": "^6.0.0",
    "@emotion/react": "^11.0.0",
    "@emotion/styled": "^11.0.0",
    "@react-oauth/google": "^0.12.0",
    "@rationalbloks/frontblok-auth": "^0.3.0",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-router-dom": "^7.0.0",
    "typescript": "^5.7.0",
    "vite": "^6.0.0",
    "vite-plugin-dts": "^4.0.0"
  },
  "keywords": [
    "react",
    "components",
    "auth",
    "frontend",
    "rationalbloks",
    "mui"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/rationalbloks/frontblok-components.git"
  }
}
